{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companyFilter.schema.json",
    "type": "object",
    "default": {},
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "title": "The name Schema",
            "examples": [
                "Social Network"
            ]
        },
        "minEmployees": {
            "type": "integer",
            "minimum": 0,
            "title": "The minEmployees Schema",
            "examples": [
                0
            ]
        },
        "maxEmployees": {
            "type": "integer",
            "minimum": 0,
            "title": "The maxEmployees Schema",
            "examples": [
                0
            ]
        }
    },
    "additionalProperties": false,
    "required": []
}
